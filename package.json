{
  "name": "openapi-path-templating",
  "version": "1.0.0",
  "description": "OpenAPI Path Templating parser, validator and resolver.",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json",
    "./path-templating.bnf": "./src/path-templating.bnf"
  },
  "scripts": {
    "grammar:compile": "./node_modules/apg-js/bin/apg.sh --in=./src/path-templating.bnf --out=./src/path-templating.js && cd ./src && mv path-templating.js path-templating.cjs",
    "test": "mocha"
  },
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/char0n/openapi-path-templating.git"
  },
  "keywords": [
    "openapi",
    "path",
    "templating",
    "parser",
    "validator",
    "resolver"
  ],
  "author": "Vladim√≠r Gorej <vladimir.gorej@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/char0n/openapi-path-templating/issues"
  },
  "homepage": "https://github.com/char0n/openapi-path-templating#readme",
  "files": [
    "src/",
    "LICENSE",
    "NOTICE",
    "package.json",
    "README.md",
    "SECURITY.md"
  ],
  "dependencies": {
    "apg-js": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "=18.4.3",
    "@commitlint/config-conventional": "=17.2.0",
    "chai": "=4.3.10",
    "husky": "=8.0.2",
    "mocha": "=10.1.0"
  }
}
